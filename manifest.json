{
  "name": "UofT Room Finder",
  "description": "Find a study room.",
  "version": "2.0",
  "permissions": [
    "activeTab",
    "<all_urls>"
  ],
  "background": {
    "scripts": ["jquery-3.1.1.min.js", "event.js"],
    "persistent": false
  },
  "browser_action": {
    "default_title": "Find a study room",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
